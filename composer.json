{
  "name": "cheese44/cheesecache",
  "description": "A request scoped cache library for PHP with the goal to make caching consistent and maintainable.",
  "type": "library",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/cheese44/cheeseCache/issues",
    "source": "https://github.com/cheese44/cheeseCache"
  },
  "keywords": ["cache", "caching", "request scope", "request scoped"],
  "require": {
    "php": ">=5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "cheeseCache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "cheeseCache\\tests\\": "tests"
    }
  }
}